<?xml version="1.0"?>
<block>
  <name>Device3</name>
  <key>variable_uhd_device3</key>
  <import>from gnuradio import uhd</import>
  <var_make>self.device3 = $(id) = uhd.device3(uhd.device_addr_t( ",".join(($dev_addr, $dev_args)) ))</var_make>
  <var_value>$dev_addr</var_value>
  <make></make>
  <param>
    <name>Device Address</name>
    <key>dev_addr</key>
    <value>type=x300</value>
    <type>string</type>
    <hide>
      \#if \$dev_addr()
      none
      \#else
      part
      \#end if
    </hide>
  </param>
  <param>
    <name>Device Arguments</name>
    <key>dev_args</key>
    <value>""</value>
    <type>string</type>
    <hide>
      \#if \$dev_args()
      none
      \#else
      part
      \#end if
    </hide>
  </param>
</block>
